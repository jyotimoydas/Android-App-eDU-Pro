<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@drawable/signup"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/editText72"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:hint="Username"
        android:textColor="#000000"
        android:fontFamily="sans-serif"
        android:drawableStart="@drawable/username"
        android:inputType="textPersonName" />

    <EditText
        android:textColor="#000000"
        android:fontFamily="sans-serif"
        android:id="@+id/editText73"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText72"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:drawableStart="@drawable/roll"
        android:hint="Roll No"
        android:inputType="textPersonName" />

    <EditText
        android:textColor="#000000"
        android:fontFamily="sans-serif"
        android:id="@+id/editText74"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/editText73"
        android:layout_alignRight="@+id/editText73"
        android:layout_below="@+id/editText73"
        android:layout_marginTop="33dp"
        android:drawableLeft="@drawable/passsss"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword" />

    <EditText
        android:textColor="#000000"
        android:fontFamily="sans-serif"
        android:id="@+id/editText75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/editText74"
        android:layout_alignStart="@+id/editText74"
        android:layout_below="@+id/editText74"
        android:layout_marginTop="27dp"
        android:ems="10"
        android:drawableStart="@drawable/barnch"
        android:hint="Branch"
        android:inputType="textPersonName" />

    <Button
        android:id="@+id/button73"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText76"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="88dp"
        android:text=" Register "
        android:textSize="30dp"
        android:fontFamily="serif-monospace"
        android:textColor="#ffffffff"
        android:background="@drawable/buttonwhite"/>

    <EditText
        android:id="@+id/editText76"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/editText75"
        android:layout_below="@+id/editText75"
        android:drawableStart="@drawable/phone"
        android:layout_marginTop="18dp"
        android:ems="10"
        android:hint="Phone"
        android:inputType="textPersonName" />
</RelativeLayout>